    "prettier": "^3.0.3",
    "puppeteer": "^21.5.0",
    "readline": "^1.3.0",
    "resemblejs": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
